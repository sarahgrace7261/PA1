{
  "expo": {
    "name": "GT-BAMA",
    "slug": "GT-BAMA",
    "owner": "mis-capstone",
    "version": "1.0.5",
    "orientation": "portrait",
    "icon": "./app/assets/gt-bama-face.png",
    "splash": {
      "image": "./app/assets/splash-screen.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.kerajeann.Capstone"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./app/assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "permissions": [
        "android.permission.MODIFY_AUDIO_SETTINGS"
      ],
      "package": "com.kerajeann.Capstone"
    },
    "web": {
      "favicon": "./app/assets/favicon.png"
    },
    "plugins": [
      "expo-av"
    ],
    "description": ""
  }
}
